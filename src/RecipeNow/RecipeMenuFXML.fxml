<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RecipeNow.RecipeMenuFXController">
   <children>
      <ListView fx:id="recipe_recipeList" layoutX="100.0" layoutY="14.0" prefHeight="250.0" prefWidth="400.0" />
      <Button fx:id="recipe_addRecipe" layoutX="240.0" layoutY="276.0" mnemonicParsing="false" onAction="#recipe_addRecipeActionPerformed" prefHeight="25.0" prefWidth="120.0" text="Create New Recipe" />
      <Button fx:id="recipe_editRecipe" layoutX="240.0" layoutY="318.0" mnemonicParsing="false" onAction="#recipe_editRecipeActionPerformed" prefHeight="25.0" prefWidth="120.0" text="Edit Recipe" />
      <Button fx:id="recipe_deleteRecipe" layoutX="240.0" layoutY="361.0" mnemonicParsing="false" onAction="#recipe_deleteRecipeActionPerformed" prefHeight="25.0" prefWidth="120.0" text="Delete Recipe" />
      <Button fx:id="recipe_saveRecipeLocal" layoutX="379.0" layoutY="275.0" mnemonicParsing="false" onAction="#recipe_saveRecipeLocalActionPerformed" prefHeight="25.0" prefWidth="197.0" text="Save Recipe To Local Storage" />
   </children>
</AnchorPane>
